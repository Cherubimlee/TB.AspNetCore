{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MySqlDbConnection": "you mysql address",
  "Persion": {
    "Name": "china",
    "Age": "22"

  },
  "applicationUrl": "http://localhost:5000",
  "Log4NetConfig": "log.config",
  "RedisConfig": {
    "RedisName": "xxx",
    "RedisHost": "xx.xxx.xx.xx:0000",
    "RedisPass": "xxxx",
    "RedisIndex": 4,
    "ConnectTimeout": 600, //异步连接等待时间
    "SyncTimeout": 600, //同步连接等待时间
